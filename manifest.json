{
  "name": "Mermaid Previewer",
  "description": "Preview mermaid. mermaid version 8.12.0.",
  "version": "1.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "128": "/icon/128.png"
    }
  },
  "icons": {
    "128": "/icon/128.png"
  }
}
